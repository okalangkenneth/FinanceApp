{
  "SendGridApiKey": "#{SendGridApiKey}#",
  "ConnectionStrings": {
    "HerokuConnection": "#{HEROKU_CONNECTION_STRING}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "FinTrak": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "FinTrak": "Error"
      }
    },
    "AllowedHosts": "*",
    "Kestrel": {
      "Endpoints": {
        "Http": {
          "Url": "http://*:8888"
        }
      }
    }
  }
}

